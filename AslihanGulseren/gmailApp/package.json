{
  "name": "gmail-app",
  "productName": "gmail-app",
  "version": "1.0.0",
  "main": "main.js",
  "description": "My Electron application description",
  "scripts": {
    "ng": "ng",
    "start": "electron-forge start",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "start:electron": "ng build --base-href ./ && electron .",
